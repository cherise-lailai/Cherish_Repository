<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="class" catalog="javaee2" class="StuClass"></table>
	<table name="check" catalog="javaee2" class="Check"></table>
	<table name="evaluation" catalog="javaee2" class="Evaluation">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
	<table name="restucourse" catalog="javaee2" class="ReStuCourse">
		<primary-key>
			<generator class="assigned"></generator>
		</primary-key>
	</table>
<table name="restudy" catalog="javaee2" class="ReStudy">
	<primary-key>
		<generator class="assigned"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>